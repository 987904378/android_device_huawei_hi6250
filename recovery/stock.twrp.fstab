/boot		emmc		/dev/block/platform/hi_mci.0/by-name/boot
/recovery       emmc            /dev/block/platform/hi_mci.0/by-name/recovery
/cache		ext4		/dev/block/platform/hi_mci.0/by-name/cache
/cust           ext4            /dev/block/platform/hi_mci.0/by-name/cust       flags=display="Cust";storage;settingsstorage;wipeingui;backup=1
/misc		emmc		/dev/block/platform/hi_mci.0/by-name/misc	flags=display="Misc";backup=0
/oeminfo        emmc            /dev/block/platform/hi_mci.0/by-name/oeminfo    flags=display="OEMinfo";backup=1
/data		ext4		/dev/block/platform/hi_mci.0/by-name/userdata	flags=length=-16384
/system		ext4		/dev/block/platform/hi_mci.0/by-name/system
/vendor		ext4		/dev/block/platform/hi_mci.0/by-name/vendor	flags=display="Vendor";backup=1
/product	ext4		/dev/block/platform/hi_mci.0/by-name/product	flags=display="Product";backup=1
/sdcard		vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="MicroSD Card";storage;wipeingui;removable
/USB-OTG	auto		/dev/block/sda1         /dev/block/sda          flags=display="USB OTG";storage;removable;backup=0



